{
  "name": "chess-database-pro",
  "version": "0.1.0",
  "description": "A comprehensive chess database application with advanced position search capabilities",
  "main": "server.js",
  "keywords": [
    "chess",
    "database",
    "pgn",
    "position-search",
    "chessbase",
    "chess-analysis",
    "sqlite",
    "express",
    "nodejs"
  ],
  "author": "Chess Database Pro Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/chess-database-pro.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/chess-database-pro/issues"
  },
  "homepage": "https://github.com/yourusername/chess-database-pro#readme",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "screenshots": "node scripts/take-screenshots.js",
    "demo": "node scripts/demo.js",
    "test": "npm run test:unit && npm run test:multipiece && npm run test:pagination && npm run test:rooks",
    "test:unit": "node tests/test-optimized-pattern-search.js",
    "test:multipiece": "node tests/test-multipiece-logic.js",
    "test:pagination": "node tests/test-pagination-pattern.js",
    "test:rooks": "node tests/test-pattern-rooks-initial.js",
    "test:api": "bash tests/api-test-commands.sh",
    "test:performance": "node tests/test-performance-benchmark.js",
    "build-index": "node scripts/build-piece-index.js"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "chess.js": "^1.0.0-beta.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "stockfish": "^16.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "puppeteer": "^24.16.2"
  }
}
